# Redirect root to frontend/index.html
RewriteEngine On

# Redirect exactly root domain to frontend/index.html
RewriteRule ^$ /frontend/index.html [L]

# Optional: Redirect without trailing slash
RewriteRule ^index\.php$ /frontend/index.html [L,R=301]

# Ensure direct access to frontend/index.html works
<Files "index.html">
    Require all granted
</Files>